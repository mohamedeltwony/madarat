wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u062e\u0644\u0627\u0644 \u0627\u0644\u0637\u0631\u064a\u0642 \u0633\u062a\u0633\u062a\u0645\u062a\u0639\u0648\u0646 \u0628\u0645\u0634\u0627\u0647\u062f\u0629 \u0628\u0639\u0636 \u0645\u0639\u0627\u0644\u0645 \u0627\u0644\u0639\u0627\u0635\u0645\u0629 \u0627\u0644\u0631\u0627\u0626\u0639\u0629. \u0628\u0639\u062f \u0627\u0644\u0648\u0635\u0648\u0644\u060c \u064a\u062a\u0645 \u0627\u0633\u062a\u0644\u0627\u0645 \u0627\u0644\u063a\u0631\u0641 \u0648\u0627\u0644\u0627\u0633\u062a\u0631\u0627\u062d\u0629 \u0641\u064a \u0627\u0644\u0641\u0646\u062f\u0642\u060c \u0648\u0645\u0646 \u062b\u0645 \u0627\u0644\u0628\u062f\u0621 \u0641\u064a \u0623\u0648\u0644\u0649 \u062c\u0648\u0644\u0627\u062a \u0645\u0648\u0633\u0643\u0648 \u0633\u064a\u0627\u062d\u0629.<\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u062c\u0648\u0644\u0629 \u0641\u064a \u0627\u0647\u0645 \u0627\u0644\u0627\u0645\u0627\u0643\u0646 \u0627\u0644\u0633\u064a\u0627\u062d\u064a\u0629 \u0641\u064a \u0645\u0648\u0633\u0643\u0648","content":"<!-- wp:paragraph -->\n<p>\u064a\u0628\u062f\u0623 \u062c\u062f\u0648\u0644 \u0633\u064a\u0627\u062d\u064a \u0641\u064a \u0645\u0648\u0633\u0643\u0648 \u0628\u0632\u064a\u0627\u0631\u0629 \u0645\u062a\u0631\u0648 \u0645\u0648\u0633\u0643\u0648 \u0627\u0644\u0634\u0647\u064a\u0631\u060c \u0627\u0644\u0645\u0639\u0631\u0648\u0641 \u0628\u062a\u0635\u0645\u064a\u0645\u0647 \u0627\u0644\u0641\u0631\u064a\u062f \u0648\u0627\u0644\u0645\u0645\u064a\u0632 \u0627\u0644\u0630\u064a \u064a\u062f\u0645\u062c \u0627\u0644\u0641\u0646 \u0648\u0627\u0644\u0639\u0645\u0627\u0631\u0629 \u0628\u0637\u0631\u064a\u0642\u0629 \u0627\u0633\u062a\u062b\u0646\u0627\u0626\u064a\u0629.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p> \u0628\u0639\u062f \u0630\u0644\u0643\u060c \u062a\u062a\u062c\u0647 \u0627\u0644\u0631\u062d\u0644\u0629 \u0625\u0644\u0649 \u0634\u0627\u0631\u0639 \u0623\u0631\u0628\u0627\u062a \u0627\u0644\u0642\u062f\u064a\u0645\u060c \u062d\u064a\u062b \u064a\u0645\u0643\u0646\u0643\u0645 \u0627\u0644\u0627\u0633\u062a\u0645\u062a\u0627\u0639 \u0628\u0627\u0644\u062a\u0635\u0627\u0645\u064a\u0645 \u0627\u0644\u0645\u0630\u0647\u0644\u0629 \u0644\u0644\u0645\u0628\u0627\u0646\u064a \u0648\u0634\u0631\u0627\u0621 \u0627\u0644\u0647\u062f\u0627\u064a\u0627 \u0627\u0644\u062a\u0630\u0643\u0627\u0631\u064a\u0629. <\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u064a\u0644\u064a \u0630\u0644\u0643 \u0627\u0644\u063a\u062f\u0627\u0621 \u0641\u064a \u0623\u062d\u062f \u0645\u0637\u0627\u0639\u0645 \u0627\u0644\u0645\u062f\u064a\u0646\u0629. <\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u0648\u0641\u064a \u0645\u062e\u0637\u0637 \u0645\u0648\u0633\u0643\u0648 \u0633\u064a\u0627\u062d\u0629\u060c \u0628\u0639\u062f \u0627\u0644\u0638\u0647\u064a\u0631\u0629 \u0646\u0642\u0648\u0645 \u0628\u0632\u064a\u0627\u0631\u0629 \u0634\u0627\u0631\u0639 \u062a\u0641\u0631\u0633\u0643\u0627\u064a\u0627 \u0648\u0627\u0644\u062c\u0627\u0644\u064a\u0631\u064a \u0627\u0644\u0634\u0647\u064a\u0631 \u0627\u0644\u0630\u064a \u064a\u0639\u0631\u0636 \u0645\u062c\u0645\u0648\u0639\u0629 \u0631\u0627\u0626\u0639\u0629 \u0645\u0646 \u0627\u0644\u0623\u0639\u0645\u0627\u0644 \u0627\u0644\u0641\u0646\u064a\u0629 \u0627\u0644\u062a\u0627\u0631\u064a\u062e\u064a\u0629 \u0648\u0627\u0644\u0645\u0639\u0627\u0635\u0631\u0629. <\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u0632\u064a\u0627\u0631\u0629 \u0627\u0644\u0633\u0627\u062d\u0629 \u0627\u0644\u062d\u0645\u0631\u0627\u0621 \u0648\u0627\u0644\u0633\u064a\u0631\u0643","content":"<!-- wp:paragraph -->\n<p>\u062c\u062f\u0648\u0644 \u0633\u064a\u0627\u062d\u064a \u0641\u064a \u0645\u0648\u0633\u0643\u0648 \u064a\u0636\u0645 \u0632\u064a\u0627\u0631\u0629 \u0627\u0644\u0633\u0627\u062d\u0629 \u0627\u0644\u062d\u0645\u0631\u0627\u0621\u060c \u0627\u0644\u062a\u064a \u064a\u0648\u062c\u062f \u0628\u0647\u0627 \u0627\u0644\u0633\u0627\u0639\u0629 \u0627\u0644\u0643\u0628\u064a\u0631\u0629 \u0627\u0644\u0634\u0647\u064a\u0631\u0629\u060c \u0627\u0644\u0642\u0628\u0628 \u0627\u0644\u0645\u0644\u0648\u0646\u0629\u060c \u0642\u0628\u0631 \u0644\u064a\u0646\u064a\u0646\u060c \u0648\u0627\u0644\u0645\u062c\u0645\u0639 \u0627\u0644\u062a\u062c\u0627\u0631\u064a \u0627\u0644\u0636\u062e\u0645. <\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u064a\u0645\u0643\u0646\u0643\u0645 \u0623\u064a\u0636\u064b\u0627 \u062f\u062e\u0648\u0644 \u0627\u0644\u0642\u0635\u0631 \u0627\u0644\u0645\u0644\u0643\u064a \u0648\u0627\u0644\u0627\u0633\u062a\u0645\u062a\u0627\u0639 \u0628\u0627\u0644\u062d\u062f\u0627\u0626\u0642 \u0627\u0644\u0631\u0626\u0627\u0633\u064a\u0629 \u0627\u0644\u062e\u0636\u0631\u0627\u0621. \u0641\u064a \u0627\u0644\u0645\u0633\u0627\u0621\u060c \u064a\u062a\u0645 \u0632\u064a\u0627\u0631\u0629 \u0627\u0644\u0633\u064a\u0631\u0643 \u0627\u0644\u0631\u0648\u0633\u064a \u0627\u0644\u0639\u0627\u0644\u0645\u064a \u0644\u0644\u0627\u0633\u062a\u0645\u062a\u0627\u0639 \u0628\u0623\u062c\u0648\u0627\u0621 \u0627\u0644\u0645\u0631\u062d.<\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u062c\u0648\u0644\u0629 \u0641\u064a \u0645\u0646\u0637\u0642\u0629 \u0646\u0648\u0641\u0648\u062f\u0648\u0641\u064a\u062a\u0634\u064a","content":"<!-- wp:paragraph -->\n<p>\u0641\u064a \u0628\u0631\u0646\u0627\u0645\u062c \u0645\u0648\u0633\u0643\u0648 \u0633\u064a\u0627\u062d\u0629\u060c \u064a\u062a\u0645 \u0632\u064a\u0627\u0631\u0629 \u0645\u0646\u0637\u0642\u0629 \u0646\u0648\u0641\u0648\u062f\u0648\u0641\u064a\u062a\u0634\u064a \u0627\u0644\u062a\u064a \u0623\u0633\u0633\u0647\u0627 \u0627\u0644\u0623\u0645\u064a\u0631 \u0641\u0627\u0633\u064a\u0644\u064a\u060c \u0648\u0647\u064a \u0627\u0647\u0645 \u0627\u0644\u0627\u0645\u0627\u0643\u0646 \u0627\u0644\u0633\u064a\u0627\u062d\u064a\u0629 \u0641\u064a \u0645\u0648\u0633\u0643\u0648.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p> \u062a\u062a\u0636\u0645\u0646 \u0627\u0644\u062c\u0648\u0644\u0629 \u0627\u0633\u062a\u0643\u0634\u0627\u0641 \u0627\u0644\u062d\u0648\u0627\u0626\u0637 \u0627\u0644\u062d\u0627\u0645\u064a\u0629\u060c \u0627\u0644\u0623\u0628\u0631\u0627\u062c\u060c \u0648\u0627\u0644\u0645\u0628\u0627\u0646\u064a \u0627\u0644\u062a\u0627\u0631\u064a\u062e\u064a\u0629 \u0627\u0644\u062a\u064a \u0634\u064a\u062f\u062a \u0644\u0644\u0623\u0645\u0631\u0627\u0621 \u0648\u0627\u0644\u062d\u0643\u0627\u0645. \u0643\u0645\u0627 \u064a\u0645\u0643\u0646 \u0632\u064a\u0627\u0631\u0629 \u0645\u0642\u0627\u0628\u0631 \u0645\u0634\u0627\u0647\u064a\u0631 \u0645\u062b\u0644 \u062a\u0634\u064a\u062e\u0648\u0641 \u0648\u0632\u0648\u062c\u0629 \u0633\u062a\u0627\u0644\u064a\u0646. <\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u062a\u0644 \u0644\u064a\u0646\u064a\u0646 \u0648\u0633\u0628\u0627\u0631\u0648","content":"<!-- wp:paragraph -->\n<p>\u062c\u062f\u0648\u0644 \u0633\u064a\u0627\u062d\u064a \u0641\u064a \u0645\u0648\u0633\u0643\u0648 \u064a\u062a\u0636\u0645\u0646 \u0632\u064a\u0627\u0631\u0629 \u062a\u0644 \u0644\u064a\u0646\u064a\u0646\u060c \u0623\u0639\u0644\u0649 \u0646\u0642\u0637\u0629 \u0641\u064a \u0645\u0648\u0633\u0643\u0648\u060c \u062d\u064a\u062b \u064a\u0645\u0643\u0646\u0643\u0645 \u0627\u0644\u0627\u0633\u062a\u0645\u062a\u0627\u0639 \u0628\u0625\u0637\u0644\u0627\u0644\u0629 \u0628\u0627\u0646\u0648\u0631\u0627\u0645\u064a\u0629 \u0631\u0627\u0626\u0639\u0629 \u0639\u0644\u0649 \u0627\u0644\u0639\u0627\u0635\u0645\u0629 \u0648\u0627\u0644\u062a\u0642\u0627\u0637 \u0623\u062c\u0645\u0644 \u0627\u0644\u0635\u0648\u0631. <\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u0628\u0639\u062f \u0630\u0644\u0643\u060c \u062a\u0646\u0627\u0648\u0644 \u0627\u0644\u063a\u062f\u0627\u0621 \u0641\u064a \u0623\u062d\u062f \u0645\u0637\u0627\u0639\u0645 \u0627\u0644\u0645\u062f\u064a\u0646\u0629 \u0642\u0628\u0644 \u0627\u0644\u0639\u0648\u062f\u0629 \u0625\u0644\u0649 \u0627\u0644\u0641\u0646\u062f\u0642. <\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u062c\u0648\u0644\u0629 \u0641\u064a \u0627\u0644\u0643\u0631\u0645\u0644\u064a\u0646 \u0645\u0646 \u0627\u0644\u062f\u0627\u062e\u0644","content":"<!-- wp:paragraph -->\n<p>\u062a\u0628\u062f\u0623 \u0627\u0644\u062c\u0648\u0644\u0629 \u0628\u0632\u064a\u0627\u0631\u0629 \u0645\u0646\u0637\u0642\u0629 \u0627\u0644\u0643\u0631\u0645\u0644\u064a\u0646 \u0627\u0644\u062a\u0627\u0631\u064a\u062e\u064a\u0629\u060c \u0627\u0644\u0645\u0639\u0631\u0648\u0641\u0629 \u0628\u0647\u0646\u062f\u0633\u062a\u0647\u0627 \u0627\u0644\u0645\u0639\u0645\u0627\u0631\u064a\u0629 \u0627\u0644\u0641\u0631\u064a\u062f\u0629 \u0648\u0623\u0647\u0645\u064a\u062a\u0647\u0627 \u0627\u0644\u0633\u064a\u0627\u0633\u064a\u0629. \u062a\u0634\u0645\u0644 \u0627\u0644\u062c\u0648\u0644\u0629 \u0623\u064a\u0636\u064b\u0627 \u0632\u064a\u0627\u0631\u0629 \u0645\u062a\u062d\u0641 \u0645\u0648\u0633\u0643\u0648 \u0627\u0644\u062a\u0627\u0631\u064a\u062e\u064a\u060c \u0648\u0643\u0627\u062a\u062f\u0631\u0627\u0626\u064a\u0629 \u0633\u064a\u0646\u062a \u0628\u0627\u0633\u064a\u0644\u060c \u0648\u0643\u0627\u062a\u062f\u0631\u0627\u0626\u064a\u0629 \u0643\u0627\u0632\u0627\u0646\u0633\u0643\u064a.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p>\u0628\u0639\u062f \u0627\u0644\u063a\u062f\u0627\u0621\u060c \u064a\u062d\u062a\u0648\u064a \u062c\u062f\u0648\u0644 \u0631\u062d\u0644\u0627\u062a \u0645\u0648\u0633\u0643\u0648 \u0633\u064a\u0627\u062d\u0629\u060c \u0632\u064a\u0627\u0631\u0629 \u0645\u0633\u0631\u062d \u0627\u0644\u0628\u0648\u0644\u0634\u0648\u064a \u0648\u0627\u0644\u0633\u064a\u0646\u0645\u0627 \u0644\u0642\u0636\u0627\u0621 \u0648\u0642\u062a \u0645\u0645\u062a\u0639. <\/p>\n<!-- \/wp:paragraph -->"},{"title":"\u064a\u0648\u0645 \u062d\u0631","content":"<!-- wp:paragraph -->\n<p>\u0647\u0630\u0627 \u0627\u0644\u064a\u0648\u0645 \u0645\u062e\u0635\u0635 \u0644\u0644\u0631\u0627\u062d\u0629 \u0648\u0627\u0644\u0627\u0633\u062a\u062c\u0645\u0627\u064