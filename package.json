    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "graphql": "^16.6.0",
    "he": "^1.2.0",
    "loader-utils": "^3.2.1",
    "next": "^14.1.0",
    "next-cloudinary": "^6.16.0",
    "nextjs-progressbar": "^0.0.16",
    "nodemailer": "^6.10.1",
    "parameterize": "^1.0.0",
