    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "glob": "^10.3.10",
        "he": "^1.2.0",
    "jwt-decode": "^4.0.0",
    "loader-utils": "^3.2.1",
    "next": "^15.3.2",
    "next-cloudinary": "^6.16.0",
    "next-csrf": "^0.2.1",
    "nextjs-progressbar": "^0.0.16",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
