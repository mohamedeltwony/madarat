  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "dev": "next dev",
    "build": "SKIP_LINT=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && prettier --write src",
