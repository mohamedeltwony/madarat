  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.0-canary.51",
        "@svgr/webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.0-canary.51",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^9.1.7",
