{
  "name": "viatour-nextjs",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production next build",
    "export": "next export",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "perf": "npm run build && npm run start",
    "size": "size-limit",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@apollo/client": "^3.12.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.17",
    "@react-google-maps/api": "^2.19.2",
    "aos": "^2.3.4",
    "axios": "^1.6.2",
    "bootstrap": "^5.1.3",
    "graphql": "^16.10.0",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-modal-video": "^2.0.1",
    "react-multi-date-picker": "^4.4.1",
    "recharts": "^2.9.3",
    "sass": "^1.83.1",
    "swiper": "^11.2.1",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@next/bundle-analyzer": "^14.2.30",
    "@size-limit/preset-big-lib": "^8.2.6",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.4",
    "size-limit": "^8.2.6",
    "web-vitals": "^5.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": ".next/static/js/**/*.js",
      "limit": "500 KB"
    },
    {
      "path": ".next/static/css/**/*.css",
      "limit": "100 KB"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
